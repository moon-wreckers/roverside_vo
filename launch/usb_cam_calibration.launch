<launch>
  <node name="usb_cam" pkg="usb_cam" type="usb_cam_node" output="screen">
    <param name="video_device" value="/dev/video0" />
    <param name="image_width" value="640" />
    <param name="image_height" value="480" />
    <param name="framerate" value="30" />
    <param name="pixel_format" value="yuyv" />
    <param name="camera_frame_id" value="usb_cam" />
    <param name="io_method" value="mmap"/>
    <param name="contrast" value="22"/>
    <param name="sharpness" value="22"/>
    <param name="brightness" value="22"/>
    <param name="saturation" value="64"/>
    <param name="gain" value="0"/>
  </node>

  <node name="camera_calibration" pkg="camera_calibration" type="cameracalibrator.py"
        args="--size 8x6 --square 0.108 image:=/usb_cam/image_raw camera:=/usb_cam">
  </node>
</launch>
